<div id="<%= dom_id vehicle %>">
  <p>
    <strong>User:</strong>
    <%= vehicle.user_id %>
  </p>

  <p>
    <strong>Brand:</strong>
    <%= vehicle.brand %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= vehicle.model %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= vehicle.year %>
