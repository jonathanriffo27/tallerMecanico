<div id="<%= dom_id vehicle %>">
  <p>
    <strong>User:</strong>
    <%= vehicle.user.name %>
  </p>
  <p>
    <strong>Brand:</strong>
    <%= vehicle.brand %>
  </p>
  <p>
    <strong>Model:</strong>
    <%= vehicle.model %>
  </p>
  <p>
    <strong>Year:</strong>
    <%= vehicle.year %>
  </p>
  <p>
    <strong>Services:</strong>
    <% vehicle.services.each do |service| %>
      <%= service.name %>
    <% end %>
  </p>
  <p>
    <strong>Replaced parts:</strong>
    <%= vehicle.replaced_parts %>
  </p>
  <p>
    <strong>Finish time:</strong>
    <%= vehicle.finish_time %>
  </p>
</div>
